    "#BMI CALCULATOR\n",
    "height = input(\"enter your height in m: \")\n",
    "weight = input(\"enter your weight in kg: \")\n",
    "\n",
    "height1 = float(height)\n",
    "weight1 = float(weight)\n",
    "BMI = weight1 / (height1 * height1)\n",
    "BMI_1 = int(BMI)\n",
    "print( BMI_1 )"
